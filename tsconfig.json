{
    "compilerOptions": {
      "target": "es5",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "outDir": "./dist"
    },
    "include": ["shared/ts/*.ts"],
    "exclude": ["node_modules"]
}